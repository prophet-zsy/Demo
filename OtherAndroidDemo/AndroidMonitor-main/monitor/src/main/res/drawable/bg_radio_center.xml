<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape>
            <solid android:color="@color/monitor_colorPrimary" />
            <stroke android:width="1dp" android:color="@color/monitor_colorPrimary" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/monitor_white" />
            <stroke android:width="1dp" android:color="@color/monitor_colorPrimary" />
        </shape>
    </item>
</selector>